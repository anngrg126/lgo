            code
              div .small-icon.primary-icon
              div &nbsp;&nbsp;span.fa.fa-comment.fa-lg
              br
              div .small-icon.primary-icon
              div &nbsp;&nbsp;span.fa.fa-thumbs-up.fa-lg
              br
              div .small-icon.primary-icon
              div &nbsp;&nbsp;span.fa.fa-heart.fa-lg
              br
              div .small-icon.primary-icon
              div &nbsp;&nbsp;span.text-stack
              div &nbsp;&nbsp;&nbsp;&nbsp;.fa-stack-1x.circle-text.filled.rotate-45 OMG
              br
              div .small-icon.primary-icon
              div &nbsp;&nbsp;span.text-stack
