div.row
  div.columns.large-6
    h4 Screenshot
  div.columns.large-6
    h4 With Current HTML/CSS/SCSS
div.row
  ul.accordion [data-accordion data-multi-expand="true" data-allow-all-closed="true"]
    li.accordion-item.is-active[data-accordion-item]
        a.accordion-title[href="#"] Icons
        /primary icons
        div.accordion-content [data-tab-content]
          h4 All Primary Icons with badges (clickable)
          div.row
            div.columns.small-12
              == link_to root_path, class: "icon-button primary-icon" do
                span.fa.fa-comment.fa-lg.fa-lg [aria-describedby="comment_count"]
                span#comment_count.icon-counter 3
              == link_to root_path, class: "icon-button primary-icon" do
                span.fa.fa-thumbs-o-up.fa-lg [aria-describedby="like_count"]
                span#like_count.icon-counter 33k
              == link_to root_path, class: "icon-button primary-icon" do
                span.fa.fa-heart.fa-lg [aria-describedby="love_count"]
                span#love_count.icon-counter 33k
              == link_to root_path, class: "icon-button primary-icon" do
                span.text-stack [aria-describedby="omg_count"]
                  .fa-stack-1x.circle-text.open.rotate-45 OMG
                span#omg_count.icon-counter 41k
              == link_to root_path, class: "icon-button primary-icon" do
                span.text-stack [aria-describedby="lol_count"]
                  .fa-stack-1x.circle-text.filled.rotate-45 LOL
                span#lol_count.icon-counter 41k
          div.row.columns
            code
              div &#61;&#61; link_to root_path, class: "icon-button primary-icon" do 
              div &nbsp;&nbsp;span.fa.fa-thumbs-o-up.fa-lg [aria-describedby="like_count"]
              div &nbsp;&nbsp;span#like_count 33k 
              br
              div &#61;&#61; link_to root_path, class: "icon-button primary-icon" do 
              div &nbsp;&nbsp;span.fa.fa-heart.fa-lg [aria-describedby="love_count"]
              div &nbsp;&nbsp;span#love_count.icon-counter 33k 
              br
              div &#61;&#61; link_to root_path, class: "icon-button primary-icon" do 
              div &nbsp;&nbsp;span.text-stack [aria-describedby="omg_count"]
              div &nbsp;&nbsp;&nbsp;&nbsp;.fa-stack-1x.circle-text.open.rotate-45 OMG
              div &nbsp;&nbsp;span#omg_count.icon-counter 41k
              br
              div &#61;&#61; link_to root_path, class: "icon-button primary-icon" do 
              div &nbsp;&nbsp;span.text-stack [aria-describedby="lol_count"
              div &nbsp;&nbsp;&nbsp;&nbsp;.fa-stack-1x.circle-text.open.rotate-45 LOL
              div &nbsp;&nbsp;span#lol_count.icon-counter 41k  
        div.accordion-content [data-tab-content]
          h4 All Primary Small Icons (not clickable, for notifications page)
          div.row
            div.columns.small-12
              .small-icon.primary-icon
