if ("<%= j form_render %>" == "user_name") {
  $("#user_name").html("<%= j render(:partial => '/dashboard/registrations/name')%>");
} else if ("<%= j form_render %>" == "user_image") {
  $("#user_photo").html("<%= j render(:partial => '/dashboard/registrations/image')%>");
} else if ("<%= j form_render %>" == "user_about_me") {
  $("#user_about_me").html("<%= j render(:partial => '/dashboard/registrations/about_me')%>");
} else if ("<%= j form_render %>" == "user_gender") {
  $("#user_gender").html("<%= j render(:partial => '/dashboard/registrations/gender')%>");
  $('input[type="radio"]').removeAttr("checked").delay(800);
};